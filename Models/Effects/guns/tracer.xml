<?xml version="1.0" encoding="ISO-8859-1"?>

<!-- Red Tracer -->
<PropertyList>
  
  <path>tracer.ac</path>

  <animation>
    <object-name>tracer</object-name>
  </animation>
  
  <animation>
    <enable-hot type="bool">false</enable-hot>
  </animation>
  
  <model>
    <name>smoke</name>
    <path>Aircraft/IL-2/Models/Effects/guns/smoke.xml</path>
    <offsets>
      <x-m> 10.00000 </x-m>
      <y-m>  0.00000 </y-m>
      <z-m> -1.43238 </z-m>
    </offsets>
  </model>

  <effect>
    <inherits-from>Effects/model-transparent</inherits-from>
    <object-name>tracer</object-name>
    <object-name>smoke</object-name>
  </effect>
  
  <animation>
    <type>dist-scale</type>
    <object-name>tracer</object-name>
    <factor> 0.001 </factor>
    <offset> 0.000 </offset>
  </animation>
  
  <animation>
    <type>material</type>
    <object-name>tracer</object-name>
    <emission>
      <factor> 1.000 </factor>
      <offset> 1.000 </offset>
      <red>    1.000 </red>
      <green>  0.333 </green>
      <blue>   0.333 </blue>
    </emission>
  </animation>
  
</PropertyList>
